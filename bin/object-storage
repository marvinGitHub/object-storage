#!/usr/bin/env php
<?php

declare(strict_types=1);

use melia\ObjectStorage\Cli\Application;

require __DIR__ . '/../vendor/autoload.php';

try {
    $app = new Application();
    exit($app->run());
} catch (Throwable $e) {
    echo sprintf('Error: %s%s%s', $e->getMessage(), PHP_EOL, $e->getTraceAsString());
}
